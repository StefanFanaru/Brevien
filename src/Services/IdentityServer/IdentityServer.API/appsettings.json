{
  "Settings": {
    "Environment": "local"
  },
  "ApplicationUrls": {
    "IdentityAPI": "https://localhost:5001",
    "AngularClient": "https://localhost:4200",
    "IdentityControl": "https://localhost:5011",
    "IdentityServerLogin": "https://localhost:5001/Account/Login",
    "Swagger": {
      "IdentityControl": "https://localhost:5011/swagger/oauth2-redirect.html"
    }
  },
  "ApplicationInsights": {
    "LogLevel": {
      "Default": "Information"
    },
    "InstrumentationKey": "???"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "identity-local"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {SourceContext}{NewLine}{Exception}{NewLine}"
        }
      }
    ]
  }
}
